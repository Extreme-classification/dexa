{
    "representation_dims": "#ARGS.embedding_dims;",
    "encoder": {
        "order": ["stransformer"],
        "stransformer": {
            "transformer": "#ARGS.encoder_name;",
            "embedding_dims": "#ARGS.embedding_dims;",
            "dropout": 0
        }
    },

    "encoder_lbl": {
        "order": ["stransformer"],
        "stransformer": {
            "transformer": "#ARGS.encoder_name;",
            "embedding_dims": "#ARGS.embedding_dims;",
            "dropout": 0
        }
    },

    "transform": {
        "order": ["identity"],
        "identity": {}
    },

    "transform_lbl": {
        "order": ["linear_aux"],
        "linear_aux": {
            "input_size": "#ARGS.embedding_dims;",
            "output_size": "#ARGS.num_labels;"
        }
    },

    "transform_doc": {
        "order": ["identity"],
        "identity": {}
    }
}
 
